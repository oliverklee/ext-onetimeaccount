<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <!--
                        The sheet title will not be visible in the BE. Still, the XML element needs to be present to
                        avoid a crash in the BE.
                    -->
                    <sheetTitle/>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.fieldsToShow>
                        <TCEforms>
                            <label>LLL:EXT:onetimeaccount/Resources/Private/Language/locallang.xlf:plugin.fieldsToShow
                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectCheckBox</renderType>
                                <minitems>1</minitems>
                                <maxitems>99</maxitems>
                                <itemsProcFunc>
                                    OliverKlee\Onetimeaccount\Configuration\FlexForms->buildFields
                                </itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.fieldsToShow>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>
